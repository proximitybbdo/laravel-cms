{
  "name": "bbdo/cms",
  "description": "BBDO CMS",
  "keywords": [
    "framework",
    "laravel",
    "cms"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "cartalyst/sentinel": "2.0.*",
    "intervention/image": "^2.4",
    "mcamara/laravel-localization": "^1.3"
  },
  "require-dev": {},
  "autoload": {
    "classmap": [
      "src/database/seeds",
      "src/database/factories"
    ],
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BBDOCms\\CmsServiceProvider"
      ]
    }
  },
  "scripts": {},
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}