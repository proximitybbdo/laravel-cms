{
  "name": "bbdo/cms",
  "description": "BBDO CMS",
  "keywords": ["framework", "laravel","cms"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "cartalyst/sentinel": "2.0.*"
  },
  "require-dev": {

  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BBDO\\Cms\\CmsServiceProvider"
      ],
      "aliases": {
        "Debugbar": "BBDO\\Cms\\Facade"
      }
    }
  },
  "scripts": {

  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
